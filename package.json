{
  "name": "n8n-nodes-xdc",
  "version": "1.0.0",
  "description": "n8n community nodes for XDC Network - Enterprise-grade blockchain with trade finance focus, XDPoS consensus, masternodes, Plugin oracles, and subnet scaling",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "xdc",
    "xdc-network",
    "xinfin",
    "blockchain",
    "trade-finance",
    "xdpos",
    "masternode",
    "defi",
    "xrc20",
    "xrc721",
    "plugin-oracle",
    "fathom-protocol",
    "subnet",
    "iso20022"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-xdc",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-xdc.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && node scripts/copy-icons.js",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/XdcNetwork.credentials.js",
      "dist/credentials/XdcScan.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Xdc/Xdc.node.js",
      "dist/nodes/Xdc/XdcTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "ethers": "^6.12.1"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
